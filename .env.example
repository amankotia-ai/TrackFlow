# Supabase configuration
# Replace these values with your actual Supabase project information
# The project ID in the URL is used for edge function calls

# You can find these values in your Supabase dashboard:
# 1. Go to https://app.supabase.com/project/_/settings/api
# 2. Look for "Project URL" and "Project API Keys"

# Example URL format: https://abcdefghijklm.supabase.co
# The part before .supabase.co is your project ID (abcdefghijklm in this example)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# UTM tracking configuration
# These settings control the UTM analytics tracking features

# Sample rate is from 0-1, where 0.05 means sample 5% of mouse movements
# Set to 0 to disable mouse movement tracking
UTM_MOUSE_TRACKING_SAMPLE_RATE=0.05

# Enable or disable click tracking (true/false)  
UTM_CLICK_TRACKING_ENABLED=true

# Debug mode for tracking - set to true to see detailed logs in console
UTM_TRACKING_DEBUG=false 